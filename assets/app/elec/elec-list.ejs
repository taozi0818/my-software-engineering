<% elecList.forEach(function (elec, index) { %>
<tr>
  <td>
    <%= index + 1 %>
  </td>
  <td>
    <%= elec.house.detailAdd %>
  </td>
  <td>
    <%= elec.expense %>
  </td>
  <td>
    <% if (elec.status === 'ENABLE') { %>
    <span class="label label-success label-xs">启用</span>
    <% } else { %>
    <span class="label label-danger label-xs">停用</span>
    <% } %>
  </td>
  <td>
    <input class="hide-id" type="hidden" value="<%= elec.id %>">
    <input class="hide-id" type="hidden" value="<%= elec.house.detailAdd %>">
    <button class="btn btn-primary btn-clear btn-xs">重置</button>
  </td>
</tr>
<% }) %>
