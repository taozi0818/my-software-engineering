<% houseList.forEach(function (house, index) { %>
<tr>
  <td>
    <%= index + 1 %>
  </td>
  <td>
    <%= house.detailAdd %>
  </td>
  <td>
    <%= house.owner.name %>
  </td>
  <td>
    <%= house.persons.length %>
  </td>
  <td>
    <a class="btn btn-primary btn-xs" href="/house/form/page#<%= house.id %>">转让</a>
  </td>
</tr>
<% }); %>
