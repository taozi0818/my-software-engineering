<% personList.forEach(function (person, index) { %>
<tr>
  <td>
    <%= index + 1 %>
  </td>
  <td>
    <%= person.name %>
  </td>
  <td>
    <%= person.sex %>
  </td>
  <td>
    <%= person.house.detailAdd %>
  </td>
  <td>
    <%= person.phone %>
  </td>
  <td>
    <input type="hidden" class="hid-id" value="<%= person.id %>">
    <button class="btn btn-danger btn-xs btn-delete" id="btn-delete">删除</button>
    <a class="btn btn-primary btn-xs" href="/persons/form/page#<%= person.id %>">编辑</a>
  </td>
</tr>
<% }); %>
